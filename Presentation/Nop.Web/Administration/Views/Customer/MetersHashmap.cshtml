                        color: data[i].meterType == "Postpaid - High Consumption" || "Solar - High Consumption" || "Solar - High Consumption" ? "red" : "blue",
                        animation: data[i].meterType == "two" ? google.maps.Animation.BOUNCE : google.maps.Animation.DROP

                    }
                });
                marker['infowindow'] = new google.maps.InfoWindow({
                    content: '<b>Customer Name: </b> ' + data[i].customerName + '<br/><b>Consumption:  </b> ' + data[i].consumption + '<br/><b>Reading: </b> ' + data[i].reading
                });
                google.maps.event.addListener(marker, 'mouseover', function () {
                    this['infowindow'].open(map, this);
                });
                google.maps.event.addListener(marker, 'mouseout', function () {
                    this['infowindow'].close(map, this);
                });
                markers.push(marker);
            }

            var markerCluster = new MarkerClusterer(map, markers, opt);
            google.maps.event.addListener(markerCluster, 'clusterclick', function (cluster) {
                var bounds = cluster.bounds_.toJSON();
                var returnedData = $.grep(data, function (element, index) {
                    return element.lattitude >= bounds.south && element.lattitude <= bounds.north && element.longitude >= bounds.west && element.longitude <= bounds.east;
                });
                console.log(returnedData)
                if (returnedData.length) {
                    createGrid(returnedData);
                } else {
                    return false;
                }

            });
            }


